{
  "name": "Emohub",
  "description": "The toolset for making github emoji art.",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.1",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions" : [
    "clipboardWrite",
    "https://a248.e.akamai.net/assets.github.com/images/icons/emoji/smiley_cat.png",
    "https://a248.e.akamai.net/assets.github.com/images/icons/emoji/poodle.png",
    "https://a248.e.akamai.net/assets.github.com/images/icons/emoji/bowtie.png"
  ],
  "icons": {
    "16": "assets/icon-128x128.png",
    "128": "assets/icon-128x128.png"
  }
}
